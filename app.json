{
  "expo": {
    "name": "Framez",
    "slug": "framez",
    "owner": "lilianobi",
    "scheme": "framez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.framez.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Framez needs access to your photos to let you share them."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.framez.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Framez needs access to your photos to let you share them."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "470a15d6-69d2-4e1d-afe8-89f1b634d3ad"
      }
    }
  }
}
